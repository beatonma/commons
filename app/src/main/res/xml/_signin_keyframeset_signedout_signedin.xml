<?xml version="1.0" encoding="utf-8"?>
<KeyFrameSet xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <!-- Signed-in views -->
    <KeyPosition
        motion:motionTarget="@+id/account_avatar"
        motion:framePosition="1"
        motion:percentX="1.0"
        motion:percentHeight="1.0"
        motion:percentWidth="1.0"
        motion:percentY="1.0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_avatar"
        motion:framePosition="1"
        android:alpha="0"/>

    <KeyPosition
        motion:motionTarget="@+id/account_username"
        motion:framePosition="1"
        motion:percentX="1.0"
        motion:percentHeight="1.0"
        motion:percentWidth="1.0"
        motion:percentY="1.0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_username"
        motion:framePosition="1"
        android:alpha="0"/>

    <KeyPosition
        motion:motionTarget="@+id/account_name_and_email"
        motion:framePosition="1"
        motion:percentX="1.0"
        motion:percentHeight="1.0"
        motion:percentWidth="1.0"
        motion:percentY="1.0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_name_and_email"
        motion:framePosition="1"
        android:alpha="0"/>

    <KeyPosition
        motion:motionTarget="@+id/account_id"
        motion:framePosition="1"
        motion:percentX="1.0"
        motion:percentHeight="1.0"
        motion:percentWidth="1.0"
        motion:percentY="1.0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_id"
        motion:framePosition="1"
        android:alpha="0"/>

    <KeyPosition
        motion:motionTarget="@+id/signout_button"
        motion:framePosition="1"
        motion:percentX="1.0"
        motion:percentHeight="1.0"
        motion:percentWidth="1.0"
        motion:percentY="1.0"/>

    <KeyAttribute
        motion:motionTarget="@+id/signout_button"
        motion:framePosition="1"
        android:alpha="0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_avatar"
        motion:framePosition="55"
        android:alpha="0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_username"
        motion:framePosition="55"
        android:alpha="0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_name_and_email"
        motion:framePosition="55"
        android:alpha="0"/>

    <KeyAttribute
        motion:motionTarget="@+id/account_id"
        motion:framePosition="55"
        android:alpha="0"/>

    <KeyAttribute
        motion:motionTarget="@+id/signout_button"
        motion:framePosition="55"
        android:alpha="0"/>


    <!-- Sign-out views -->
    <KeyPosition
        motion:motionTarget="@+id/rationale"
        motion:framePosition="99"
        motion:percentX="0"
        motion:percentHeight="0"
        motion:percentWidth="0"
        />

    <KeyAttribute
        motion:motionTarget="@+id/rationale"
        motion:framePosition="45"
        android:alpha="0"/>


    <KeyPosition
        motion:motionTarget="@+id/g_sign_in_button"
        motion:framePosition="99"
        motion:percentX="0"
        motion:percentHeight="0"
        motion:percentWidth="0"
        />

    <KeyAttribute
        motion:motionTarget="@+id/g_sign_in_button"
        motion:framePosition="45"
        android:alpha="0"/>

</KeyFrameSet>
