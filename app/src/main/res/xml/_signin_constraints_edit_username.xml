<?xml version="1.0" encoding="utf-8"?>
<injected xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <Constraint
        android:id="@+id/account_avatar"
        android:layout_width="@dimen/user_avatar_size_large"
        android:layout_height="@dimen/user_avatar_size_large"
        android:layout_margin="@dimen/user_avatar_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top_guideline"
        android:visibility="visible"
        />

    <Constraint
        android:id="@+id/account_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/flow_gap_large"
        android:textSize="@dimen/text_size_h1"
        app:layout_constraintHeight_min="@dimen/touchable_min_size"
        app:layout_constraintTop_toBottomOf="@+id/account_avatar"
        app:layout_constraintStart_toEndOf="@+id/start_guideline"
        app:layout_constraintEnd_toStartOf="@+id/edit_username"
        android:visibility="invisible"
        />

    <Constraint
        android:id="@+id/account_username_editable_wrapper"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_h1"
        app:layout_constraintHeight_min="@dimen/touchable_min_size"
        app:layout_constraintTop_toTopOf="@+id/account_username"
        app:layout_constraintStart_toEndOf="@+id/start_guideline"
        app:layout_constraintEnd_toStartOf="@+id/submit_new_username_button"
        android:visibility="visible"
        />

    <Constraint
        android:id="@+id/edit_username_icon"
        android:layout_width="@dimen/touchable_icon_size"
        android:layout_height="@dimen/touchable_icon_size"
        app:layout_constraintTop_toTopOf="@+id/account_username"
        app:layout_constraintBottom_toBottomOf="@+id/account_username"
        app:layout_constraintStart_toEndOf="@+id/account_username"
        android:visibility="gone"
        />

    <Constraint
        android:id="@+id/submit_new_username_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/account_username"
        app:layout_constraintBottom_toBottomOf="@+id/account_username"
        app:layout_constraintEnd_toStartOf="@+id/end_guideline"
        android:visibility="visible"
        />

    <Constraint
        android:id="@+id/account_name_and_email"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/account_username_editable_wrapper"
        app:layout_constraintStart_toEndOf="@+id/start_guideline"
        app:layout_constraintEnd_toStartOf="@+id/end_guideline"
        android:visibility="visible"
        />

    <Constraint
        android:id="@+id/account_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/account_name_and_email"
        app:layout_constraintStart_toEndOf="@+id/start_guideline"
        app:layout_constraintEnd_toStartOf="@+id/end_guideline"
        />

    <Constraint
        android:id="@+id/button_spacer"
        android:layout_height="@dimen/flow_gap_large"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/account_id"
        />

    <Constraint
        android:id="@+id/delete_account_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/start_guideline"
        app:layout_constraintEnd_toStartOf="@+id/signout_button"
        app:layout_constraintTop_toBottomOf="@+id/button_spacer"
        android:visibility="invisible"/>


    <Constraint
        android:id="@+id/signout_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/delete_account_button"
        app:layout_constraintStart_toEndOf="@+id/delete_account_button"
        app:layout_constraintEnd_toEndOf="@+id/end_guideline"
        android:visibility="invisible"
        />

    <Constraint
        android:id="@+id/rationale"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone"
        />

    <Constraint
        android:id="@+id/g_sign_in_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone"
        />

