        motion:framePosition="50"
        motion:percentWidth="0.0"
        motion:percentHeight="1.0"
        motion:percentX="-0.4"
        motion:percentY="1.0"
        />

    <KeyPosition
        motion:motionTarget="@+id/account_id"
        motion:framePosition="50"
        motion:percentWidth="0.0"
        motion:percentHeight="1.0"
        motion:percentX="-0.2"
        motion:percentY="1.0"
        />

    <KeyPosition
        motion:motionTarget="@+id/signout_button"
        motion:framePosition="50"
        motion:percentX="1.0"
        motion:percentY="1.0"
        />

    <KeyPosition
        motion:motionTarget="@+id/delete_account_button"
        motion:framePosition="1"
        motion:percentWidth="1.0"
        motion:percentHeight="1.0"
        motion:percentX="1.0"
        motion:percentY="1.0"
        />
    
    
    
    

