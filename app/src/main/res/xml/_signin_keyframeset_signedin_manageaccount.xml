<?xml version="1.0" encoding="utf-8"?>
<KeyFrameSet xmlns:motion="http://schemas.android.com/apk/res-auto">

    <KeyPosition
        motion:motionTarget="@+id/open_full_account"
        motion:framePosition="30"
        motion:percentY="0.8"/>

    <KeyPosition
        motion:motionTarget="@+id/barrier"
        motion:framePosition="30"
        motion:percentY="0.8"/>

    <KeyPosition
        motion:motionTarget="@+id/background"
        motion:framePosition="30"
        motion:percentHeight="0.8"/>
    

    <KeyPosition
